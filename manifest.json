{
    "manifest_version": 3,
    "name": "Nba Extension",
    "version": "0.1",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["scripts/content.js"]
      }
    ],
    "action": {
      "default_icon": "images/nba-2-icon.png",
      "default_popup": "popup/popup.html" 
    }
}

